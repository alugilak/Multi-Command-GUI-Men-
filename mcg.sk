on join:
	set {_player} to player
	wait 10 tick	 
	set slot 7 of player to {@lager} named "{@lager-name}"  	
	format slot 7 of player with {@lager} named "{@lager-name}" with lore "{@lager-lore}" to be unstealable
	wait 10 tick
	set slot 8 of player to {@guiitem} named "{@gui-name}"
	format slot 8 of player with {@guiitem} named "{@gui-name}" with lore "{@gui-lore}" to be unstealable
	set slot 6 of player to {@join} named "{@name}"
	format slot 6 of player with {@join} named "{@name}" with lore "{@lore}" to close then run [execute player command "{@teleport}"]
	execute player command "{@teleport}"
	set the player's tablist name to "&c &f%name of player%"
	set tab header to "{@header}" and footer to "{@footer}" for player
	wait 10 second
	broadcast "{@farbcode19}%player% {@farbcode7}hat {@belohungsnachricht}"
	message "{@tabnachricht1}"	
	message "{@contxt0}" 
	message "{@contxt1}" 
	message "{@contxt2}" 
	message "{@contxt3}" 
	message "{@contxt4}"
	message "{@contxt5}"

	
